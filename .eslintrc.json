{
  "extends": "airbnb-base",
  "rules": {
    "consistent-return": 1,
    "no-unused-vars": 1,
    "no-underscore-dangle": 1,
    "operator-linebreak": 1,
    "object-curly-newline": 1,
    "comma-dangle": 0,
    "arrow-parens": 0,
    "implicit-arrow-linebreak": 0,
    "function-paren-newline": 0,
    "linebreak-style": 0
  }
}
